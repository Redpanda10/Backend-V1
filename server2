const express = require('express')

const app= express()

require("dotenv").config()


const connectDB = require('./db')

connectDB()

app.get('/',(req,res)=>{
    res.send('Hello World')
})

const PORT = process.env.PORT || 8000 

app.listen(PORT,()=>{
    console.log(`server is running on port ${PORT}`)
})
